<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>

<!--<?import javafx.scene.web.*?>-->

<VBox prefHeight="440.0" prefWidth="595.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.module3.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <GridPane hgap="8.0" prefHeight="98.0" prefWidth="653.0" vgap="20.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" minWidth="10.0" prefWidth="77.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="274.0" minWidth="10.0" prefWidth="272.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextField fx:id="localCode" prefHeight="25.0" prefWidth="262.0" GridPane.columnIndex="1" />
         <TextField fx:id="iataCode" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <TextField fx:id="ident" prefHeight="25.0" prefWidth="398.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />

        <Label fx:id="welcomeText" alignment="TOP_LEFT" contentDisplay="CENTER" prefHeight="17.0" prefWidth="74.0" text="IATA Code:" GridPane.rowIndex="1" />
         <Label prefHeight="17.0" prefWidth="65.0" text="Local Code:" />
         <Label prefHeight="17.0" prefWidth="55.0" text="Ident:" GridPane.rowIndex="2" />
      </children>
   </GridPane>
  <Button id="search" defaultButton="true" onAction="#onHelloButtonClick" text="Search" textAlignment="CENTER" underline="true" VBox.vgrow="NEVER">
      <font>
         <Font name="System Italic" size="12.0" />
      </font></Button>
   <GridPane hgap="8.0" prefHeight="179.0" prefWidth="451.0" vgap="20.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="170.0" minWidth="10.0" prefWidth="81.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.0" minWidth="10.0" prefWidth="268.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextField fx:id="type" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="0" GridPane.vgrow="NEVER" />
         <TextField fx:id="name" prefHeight="20.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.vgrow="NEVER" />
         <TextField fx:id="elevation" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="2" GridPane.vgrow="NEVER" />
         <TextField fx:id="country" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="3" GridPane.vgrow="NEVER" />
         <TextField fx:id="region" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="4" GridPane.vgrow="NEVER" />
         <TextField fx:id="municipality" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="5" GridPane.vgrow="NEVER" />
         <Label text="Type:" />
         <Label text="Name:" GridPane.rowIndex="1" />
         <Label text="Elevation:" GridPane.rowIndex="2" />
         <Label text="Country:" GridPane.rowIndex="3" />
         <Label text="Region:" GridPane.rowIndex="4" />
         <Label text="Municipality:" GridPane.rowIndex="5" />
      </children>
   </GridPane>
   <WebView fx:id="weatherWebView" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" />
</VBox>
